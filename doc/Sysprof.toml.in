[library]
version = "@version@"
browse_url = "https://gitlab.gnome.org/GNOME/sysprof/"
repository_url = "https://gitlab.gnome.org/GNOME/sysprof.git"
website_url = "https://gitlab.gnome.org/GNOME/sysprof/"
docs_url = "https://gnome.pages.gitlab.gnome.org/sysprof/"
authors = "Christian Hergert"
license = "GPL-3.0-or-later"
description = "A GNOME library providing profiling capabilities"
dependencies = ["Gio-2.0", "Dex-1", "GLib-2.0", "GObject-2.0"]
devhelp = true
search_index = true

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/gobject/"

  [dependencies."GLib-2.0"]
  name = "GLib"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/glib/"

  [dependencies."Gio-2.0"]
  name = "GIO"
  description = "GObject Interfaces and Objects, Networking, IPC, and I/O"
  docs_url = "https://docs.gtk.org/gio/"

  [dependencies."Dex-1"]
  name = "Dex-1"
  description = "Future-based programming for GNOME"
  docs_url = "https://gnome.pages.gitlab.gnome.org/libdex/"

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

[source-location]
base_url = "https://gitlab.gnome.org/GNOME/sysprof/-/blob/master/"

[extra]
# The same order will be used when generating the index
content_files = [
  "overview.md",
]

urlmap_file = "urlmap.js"
