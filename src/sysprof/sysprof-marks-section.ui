                            <property name="title" translatable="yes">Maximum</property>
                            <property name="sorter">
                              <object class="GtkNumericSorter">
                                <property name="expression">
                                  <lookup name="max-duration" type="SysprofMarkCatalog"/>
                                </property>
                              </object>
                            </property>
                            <property name="factory">
